multi:
type: txt
help: IPv4 or IPv6 address of the remote peer
val_help: ipv4; IPv4 address of the remote peer
val_help: ipv6; IPv6 address of the remote peer
val_help: hostname; Fully qualified domain name of the remote peer
val_help: any; Allow any IP address of the remote peer
syntax:expression: exec "${vyos_libexec_dir}/validate-value --regex \'(any)\' --exec \"${vyos_validators_dir}/ipv4-address \" --exec \"${vyos_validators_dir}/ipv6-address \" --exec \"${vyos_validators_dir}/fqdn \"  --value \'$VAR(@)\'"; "Invalid value"
