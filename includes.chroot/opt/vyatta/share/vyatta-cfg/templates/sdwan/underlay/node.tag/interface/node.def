multi:
type: txt
help: Interface to use for this Circuit
val_help: txt; Interface name
allowed: sh -c "${vyos_completion_dir}/list_interfaces"
syntax:expression: exec "${vyos_libexec_dir}/validate-value --regex \'(bond|br|dum|en|eth|wlan|wwan)[0-9]+(.\\d+)?|lo\' --exec \"${vyos_validators_dir}/file-path --lookup-path /sys/class/net --directory \"  --value \'$VAR(@)\'"; "Invalid value"
