multi:
type: txt
help: Interface name state check
val_help: txt; Interface name
allowed: sh -c "${vyos_completion_dir}/list_interfaces --broadcast"
syntax:expression: exec "${vyos_libexec_dir}/validate-value --regex \'(bond|br|dum|en|ersp|eth|gnv|ifb|lan|l2tp|l2tpeth|macsec|peth|ppp|pppoe|pptp|sstp|tun|veth|vti|vtun|vxlan|wg|wlan|wwan)[0-9]+(.\\d+)?|lo\' --exec \"${vyos_validators_dir}/file-path --lookup-path /sys/class/net --directory \"  --value \'$VAR(@)\'"; "Invalid value"
