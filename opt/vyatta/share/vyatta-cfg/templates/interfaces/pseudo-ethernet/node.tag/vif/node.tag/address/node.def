multi:
type: txt
help: IP address
val_help: ipv4net; IPv4 address and prefix length
val_help: ipv6net; IPv6 address and prefix length
val_help: dhcp; Dynamic Host Configuration Protocol
val_help: dhcpv6; Dynamic Host Configuration Protocol for IPv6
allowed: echo "dhcp dhcpv6"
syntax:expression: exec "${vyos_libexec_dir}/validate-value --regex \'(dhcp|dhcpv6)\' --exec \"${vyos_validators_dir}/ip-host \"  --value \'$VAR(@)\'"; "Invalid value"
