tag:
type: txt
help: "AAAA" record
val_help: txt; A DNS name relative to the root record
val_help: @; Root record
val_help: any; Wildcard record (any subdomain)
syntax:expression: exec "${vyos_libexec_dir}/validate-value --regex \'([-_a-zA-Z0-9.]{1,63}|@|any)(?<!\\.)\'   --value \'$VAR(@)\'"; "Invalid value"
